{
  "name": "react_universal",
  "version": "1.0.0",
  "description": "Find all countries detail in one place.",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "clean:all": "npm run analyze:clean && npm run bundle:clean",
    "bundle:clean": "rimraf ./build",
    "presetup": "npm i chalk shelljs",
    "setup": "npm run presetup && npm i",
    "dev": "cross-env NODE_ENV=development node server",
    "start": "npm run build && cross-env NODE_ENV=production node server",
    "lint": "eslint src",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maheshambiga/universal_react.git"
  },
  "author": "Mahesh Ambig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maheshambiga/universal_react/issues"
  },
  "homepage": "https://github.com/maheshambiga/universal_react#readme",
